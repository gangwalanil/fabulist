    function displayCountries(countryname) {
        divClassIdArr = Array('america', 'asia', 'australia', 'europe');
        for (i = 0; i < divClassIdArr.length; i++) {
            if (divClassIdArr[i] == 'europe') {
                document.getElementById(divClassIdArr[i] + '_countries_left').style.display = 'none';
                document.getElementById(divClassIdArr[i] + '_countries_right').style.display = 'none';
            }
            else {
                document.getElementById(divClassIdArr[i] + '_countries').style.display = 'none';
            }
            document.getElementById(divClassIdArr[i] + '_continent').style.color = '#8f8f8f';
        }
        if (countryname == 'europe') {
            document.getElementById(countryname + '_countries_left').style.display = 'block';
            document.getElementById(countryname + '_countries_right').style.display = 'block';
        }
        else {
            document.getElementById(countryname + '_countries').style.display = 'block';
        }
        document.getElementById(countryname + '_continent').style.color = '#000000';
    }
     jQuery(window).load(function() {
            displayCountries('australia')
        });
